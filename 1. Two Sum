class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i,j;
        int a[]=new int[2];
       // int n=nums.length();
        int sum=0;
        for(i=0;i<nums.length;i++){
            for(j=i+1;j<nums.length;j++){
                sum=nums[i]+nums[j];
                if(sum==target){
                    a[0]=i;
                    a[1]=j;
                }
            }
        }
        return a;
        
    }
}
