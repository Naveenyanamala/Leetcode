class Solution {
    public String reverseWords(String s) {
        StringBuilder res=new StringBuilder();
        String[] word=s.split(" ");
        for(int i=word.length-1;i>=0;i--){
            if(word[i]!=""){
            res.append(word[i]);
            res.append(" ");
            }
        }
        return res.toString().trim();
    }
}
